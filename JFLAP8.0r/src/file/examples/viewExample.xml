					<structure type="vertices">
						<vertex>
							<id>0</id>
							<x>115</x>
							<y>123</y>
						</vertex>
						
						<vertex>
							<id>1</id>
							<x>120</x>
							<y>112</y>
						</vertex>
					</structure>
					
					<!-- The set of edges-->
					<structure type="edges">
						<edge>
							<from>0</from>
							<to>0</to>
							<ctrl_x>100</ctrl_x>
							<ctrl_y>125</ctrl_y>
						</edge>
						
						<edge>
							<from>0</from>
							<to>1</to>
							<ctrl_x>105</ctrl_x>
							<ctrl_y>122</ctrl_y>
						</edge>
					</structure>
				</structure>
					
			</structure>
			
		<tab>
			<index>1</index>
