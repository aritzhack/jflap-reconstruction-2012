<!--View Format:-->
<root>
	<view>
		<tab>
			<index>0</index>
			<structure type="fsa_editor">
			
				<!-- THE AUTOMATON FORMAL DEFINITION -->
				<structure type="fsa">
				
					<!-- The State Set -->
					<structure type="state_set">
						<state>
							<id>0</id>
							<name>"q0"</name>
						<state>
							<id>1</id>
							<name>"q1"</name>
					</structure>
					
					<!-- The Input Alpabet -->
					<structure type="alphabet">
						<alphtype>input</alphtype>
						<symbol>a</symbol>
						<symbol>b</symbol>
						<symbol>c</symbol>
					</structure>
					
					<!-- The Start State -->
					<structure type="start_state">
						<state>0</state>
					</structure>
					
